{
  "cli": {
    "version": ">= 16.2.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "development_tv": {
      "extends": "development",
      "env": {
        "EXPO_TV": "1"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "preview_tv": {
      "extends": "preview",
      "env": {
        "EXPO_TV": "1"
      }
    },
    "production": {
      "autoIncrement": true,
      "extends": "apk",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "image": "latest"
      }
    },
    "production_tv": {
      "extends": "production",
      "env": {
        "EXPO_TV": "1"
      }
    },
    "release": {
      "distribution": "store",
      "android": {
        "buildType": "app-bundle"
      }
    },
    "release_tv": {
      "extends": "release",
      "env": {
        "EXPO_TV": "1"
      }
    },
    "apk": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      }
    },
    "apk_tv": {
      "extends": "apk",
      "env": {
        "EXPO_TV": "1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
